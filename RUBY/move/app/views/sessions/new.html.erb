<div class="row">
  <div class="col-lg-4 col-md-3">
  </div>
  <div class="col-lg-4 col-md-6">
    <h1>Sign In</h1>

    <%= simple_form_for(:session, url: sessions_path) do |f| %>
      <%= f.input :email %>
      <%= f.input :password %>

      <%= f.button :submit, "Sign In" %>
    <% end %>
  </div>
  <div class="col-lg-4 col-md-3">
  </div>
</div>
